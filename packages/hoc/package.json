    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@gaopeng123/error-page": "workspace:^0.4.18",
    "@gaopeng123/utils.function": "latest",
    "@gaopeng123/utils.types": "latest",
    "history": "^5.3.0",
    "react-activation": "^0.11.2",
