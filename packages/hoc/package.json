      "pre-commit": "tsdx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ligaopeng123-npm/hoc.git"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/hoc.esm.js",
  "size-limit": [
    {
      "path": "dist/hoc.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hoc.esm.js",
      "limit": "10 KB"
    }
