    "@types/react-router-dom": "^5.1.8",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.12",
    "cz-conventional-changelog": "^3.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.1",
    "less-loader": "^11.0.0",
    "postcss-url": "^10.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^5.0.1",
